@import 'style/weui.wxss';

page{
    background-color: #F8F8F8;
    font-size: 16px;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 12px;
    padding-right: 12px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}

.gbox{
    border-radius:5px;
    box-shadow: 5px 5px 1px rgba(98, 94, 221, 0.02);
}